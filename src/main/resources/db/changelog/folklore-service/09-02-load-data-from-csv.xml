<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="load-data-folklore" author="rassafel">
        <loadData tableName="building"
                  file="db/csv/building.csv"
                  usePreparedStatements="true">
            <column name="id" type="numeric"/>
            <column name="name" type="string"/>
            <column name="lat" type="numeric"/>
            <column name="lon" type="numeric"/>
            <column name="address" type="string"/>
            <column name="image_url" type="string"/>
            <column name="label_url" type="string"/>
            <column name="description" type="string"/>
            <column name="media_urls" type="string"/>
        </loadData>

        <loadData tableName="building_tag"
                  file="db/csv/building_tag.csv"
                  usePreparedStatements="true">
            <column name="building_id" type="numeric"/>
            <column name="tag_id" type="numeric"/>
        </loadData>
    </changeSet>
</databaseChangeLog>